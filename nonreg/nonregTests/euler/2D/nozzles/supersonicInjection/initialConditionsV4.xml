<?xml version = "1.0" encoding = "UTF-8" standalone = "yes"?>
<CI>
	<!-- LIST OF GEOMETRICAL DOMAINS  -->
	<physicalDomains>	
		<domain name="base" state="around" type="entireDomain"/>
	</physicalDomains>	

	<!-- LIST OF BOUNDARY CONDITIONS -->
	<boundaryConditions>
		<boundCond name="axis" type="wall" number="1"/>
		<boundCond name="wall" type="wall" number="2"/>
		<boundCond name="entrance" type="injection" number="3">
		    <dataInjection m0="800."/>
		    <dataFluid EOS="IG_helium.xml" density="2.0" pressure="1.5e5"/>
		</boundCond>
		<boundCond name="exit" type="outflow" number="4">
			<dataOutflow p0="1.e5"/>
		</boundCond>	
	</boundaryConditions>

	
	<!--  LIST OF STATES  -->
	<state name="around">
		<material type="fluid" EOS="IG_helium.xml">
			<dataFluid density="1.0" pressure="1.e5">
				<velocity x="0." y="0." z="0."/>
			</dataFluid>
		</material>
	</state>

</CI>
